<Window x:Class="TestSharePointBrowserAuthWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestSharePointBrowserAuthWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <DockPanel DockPanel.Dock="Top">
            <Button Content="Go" Name="GetSharePointButton" Click="GetSharePointButton_Click" />
            <TextBlock Text="Site Url: " DockPanel.Dock="Left" />
            <TextBox Name="SiteUrlTextBox" DockPanel.Dock="Left" />
        </DockPanel>
        <TextBlock DockPanel.Dock="Bottom" Name="OutputTextBox" />
    </DockPanel>
</Window>
